#include <stdio.h>
#include <stdlib.h>
#include <strings.h>

struct mta { char* train; int delay; }; // creates struct prototype

int values(struct x){
  printf("train name, %s\n" x.train);
  printf("delays, %d\n", x.delay);
  return 0;
}

int main(){
  struct mta a;
  a.train = "n";
  a.delay = 15;
  values(a);
  return 0;
}
